{
    "name": "cayeye/whatsapp-business-api-client",
    "type": "library",
    "description": "A HTTP client for the WhatsApp Business API",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "symfony/mime": "^4.3",
